<h1><%= "add photo to gallery:"[:add_photo_to_gallery] %></h1>
<ul>
  <% @images.each do |asset| %>
    <li>
      <%= thumbnail_img_tag(asset, :small) %>
      <%= link_to asset.filename, page_xurl(@page, :action => 'add', :id => asset.id), :method => :post %>
    </li>
  <% end %>
</ul>

<%= pagination_links(@images) %>

<p>
  <%= link_to 'cancel'[:cancel_link], page_url(@page) %>
</p>
