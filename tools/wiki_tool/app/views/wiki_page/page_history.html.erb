<% render :partial => 'tabs' %>

<table class="pretty">
  <tr>
    <th><%= "date"[:date] %></th>
    <th><%= "action"[:action] %></th>
  </tr>

  <% @page.page_history.each do |ph| %>
    <tr class="<%= cycle('odd','even') %>">
      <td><%= full_time(ph.created_at) %></td>
      <td><%= description_for(ph) %></td>
    </tr>
  <% end %>
</table>
