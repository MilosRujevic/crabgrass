<% @title_box = content_tag :h1, 'Search'[:search_headline] %>
<% content_for :html_head do %>
  <link rel="alternate" href="<%= url_for group_search_url(*current_rss_path) %>" title="<%= "RSS Feed"[:rss_feed] %>" type="application/rss+xml" />
<% end %>

<%= render :partial => 'pages/search', :locals => {:users => @visible_users} %>

<%= render :partial => 'pages/list', :locals => {:heading => true} %>

