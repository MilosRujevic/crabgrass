%section#group-logo
  = avatar_for @group, 'big'
%section#contribute
  %h2 
    = I18n.t(:contribute_content_link)
%section#group-meta
  %section#about-group
    %h3 
      = I18n.t(:about)
    %p
      = text_with_more( @group.profiles.public.summary_html) do
        %a{:href => "#"} Read More
    = render :partial => '/layouts/base/group_details_links'
  %section#people
    = render :partial => '/layouts/base/people'
  %section#affiliations
  %section#featured
  = render :partial => '/layouts/base/featured'

